<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ksj.myboard.evaluation">
    <select id="selectEvaluationByBoardNoAndEmail" resultType="com.ksj.myboard.vo.EvaluationVO" parameterType="hashMap">
        SELECT *
        FROM EVALUATION
        WHERE USER_EMAIL=#{userEmail} AND
              BOARD_NO=#{boardNo}
    </select>
</mapper>
